/**
 * Same as CROP_TRANSPARENT
 * 
 * @type number
 */
var CROP_DEFAULT = 0


/**
 * Crop using the transparent color
 * 
 * @type number
 */
var CROP_TRANSPARENT = 1


/**
 * Crop black borders
 * 
 * @type number
 */
var CROP_BLACK = 2


/**
 * Crop white borders
 * 
 * @type number
 */
var CROP_WHITE = 3


/**
 * Crop using colors of the 4 corners
 * 
 * @type number
 */
var CROP_SIDES = 4
