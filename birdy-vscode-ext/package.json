{
    "name": "bird",
    "displayName": "Bird",
    "description": "Support for the Bird programming language",
    "author": "Ore Richard Muyiwa",
    "icon": "icon.png",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
