{
	"fileTypes": [
		"birdy"
	],
	"foldingStartMarker": "\\{\\s*$",
	"foldingStopMarker": "^\\s*\\}",
	"name": "Birdy",
	"patterns": [
		{
			"begin": "^\\w*\\b(import)\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.other.import.birdy"
				}
			},
			"end": "\n",
			"endCaptures": {
				"0": {
					"name": "punctuation.terminator.birdy"
				}
			},
			"name": "meta.declaration.birdy",
			"patterns": [
				{
					"include": "#strings"
				},
				{
					"include": "#comments"
				},
				{
					"match": "\\b(as)\\b",
					"name": "keyword.other.import.birdy"
				}
			]
		},
		{
			"include": "#comments"
		},
		{
			"include": "#punctuation"
		},
		{
			"include": "#annotations"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#constants-and-special-vars"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"birdydoc": {
			"patterns": [
				{
					"match": "(@[a-zA-Z_]+)",
					"captures": {
						"0": {
							"name": "variable.name.source.birdy"
						}
					}
				},
				{
					"match": "(`.*?`)",
					"captures": {
						"0": {
							"name": "variable.other.source.birdy"
						}
					}
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"captures": {
						"0": {
							"name": "punctuation.definition.comment.birdy"
						}
					},
					"match": "@==@",
					"name": "comment.block.empty.birdy"
				},
				{
					"include": "#comments-doc-oldschool"
				},
				{
					"include": "#comments-doc"
				},
				{
					"include": "#comments-inline"
				}
			]
		},
		"comments-doc-oldschool": {
			"patterns": [
				{
					"begin": "@=",
					"end": "=@",
					"name": "comment.block.documentation.birdy",
					"patterns": [
						{
							"include": "#comments-doc-oldschool"
						},
						{
							"include": "#comments-block"
						},
						{
							"include": "#birdydoc"
						}
					]
				}
			]
		},
		"comments-doc": {
			"patterns": [
				{
					"begin": "#",
					"while": "^\\s*#",
					"patterns": [
						{
							"include": "#birdydoc"
						}
					],
					"name": "comment.block.documentation.birdy"
				}
			]
		},
		"comments-inline": {
			"patterns": [
				{
					"include": "#comments-block"
				},
				{
					"captures": {
						"1": {
							"name": "comment.line.double-slash.birdy"
						}
					},
					"match": "((#).*)$"
				}
			]
		},
		"comments-block": {
			"patterns": [
				{
					"begin": "@=",
					"end": "=@",
					"name": "comment.block.birdy",
					"patterns": [
						{
							"include": "#comments-block"
						}
					]
				}
			]
		},
		"annotations": {
			"patterns": [
				{
					"match": "@",
					"name": "storage.type.annotation.birdy"
				}
			]
		},
		"constants-and-special-vars": {
			"patterns": [
				{
					"match": "(?<!\\$)\\b(true|false|nil)\\b(?!\\$)",
					"name": "constant.language.birdy"
				},
				{
					"match": "((?<!\\$)\\b(self|parent)\\b(?!\\$)|(__[a-zA-Z_0-9]+__))",
					"name": "variable.language.birdy"
				},
				{
					"match": "(?<!\\$)\\b((0(x|X)[0-9a-fA-F]*)|(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)\\b(?!\\$)",
					"name": "constant.numeric.birdy"
				},
				{
					"match": "(?<![a-zA-Z0-9_$])([_$]*[A-Z][a-zA-Z0-9_$]*|bool\\b|num\\b|int\\b|double\\b|dynamic\\b)",
					"name": "support.class.birdy"
				},
				{
					"match": "([_$]*[a-z][a-zA-Z0-9_$]*)(<|\\(|\\s+=>)",
					"captures": {
						"1": {
							"name": "entity.name.function.birdy"
						}
					}
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"match": "(?<!\\$)\\bas\\b(?!\\$)",
					"name": "keyword.cast.birdy"
				},
				{
					"match": "(?<!\\$)\\b(try|on|catch|finally|throw|rethrow)\\b(?!\\$)",
					"name": "keyword.control.catch-exception.birdy"
				},
				{
					"match": "(?<!\\$)\\b(break|when|continue|default|else|for|if|in|return|using|while|iter)\\b(?!\\$)",
					"name": "keyword.control.birdy"
				},
				{
					"match": "(?<!\\$)\\b(or|and|assert)\\b(?!\\$)",
					"name": "keyword.control.boolean.birdy"
				},
				{
					"match": "(?<!\\$)\\b(class|def)\\b(?!\\$)",
					"name": "keyword.declaration.birdy"
				},
				{
					"match": "(?<!\\$)\\b(is\\!?)\\b(?!\\$)",
					"name": "keyword.operator.birdy"
				},
				{
					"match": "\\?\\?",
					"name": "keyword.operator.conditional.birdy"
				},
				{
					"match": "(<<|>>>?|~|\\^|\\||&)",
					"name": "keyword.operator.bitwise.birdy"
				},
				{
					"match": "((&|\\^|\\||<<|>>?)=)",
					"name": "keyword.operator.assignment.bitwise.birdy"
				},
				{
					"match": "(=>)",
					"name": "keyword.operator.closure.birdy"
				},
				{
					"match": "(==|!=|<<?=?|>>?=?|(\\.\\.\\.))",
					"name": "keyword.operator.comparison.birdy"
				},
				{
					"match": "(([+*/%-]|\\~)=)",
					"name": "keyword.operator.assignment.arithmetic.birdy"
				},
				{
					"match": "(=)",
					"name": "keyword.operator.assignment.birdy"
				},
				{
					"match": "(\\-\\-|\\+\\+)",
					"name": "keyword.operator.increment-decrement.birdy"
				},
				{
					"match": "(\\-|\\+|\\*|\\/|\\~\\/|%|\\*\\*)",
					"name": "keyword.operator.arithmetic.birdy"
				},
				{
					"match": "(!)",
					"name": "keyword.operator.binary.birdy"
				},
				{
					"match": "(?<!\\$)\\b(static)\\b(?!\\$)",
					"name": "storage.modifier.birdy"
				},
				{
					"match": "(?<!\\$)\\b(?:var|echo|die)\\b(?!\\$)",
					"name": "storage.type.primitive.birdy"
				}
			]
		},
		"string-interp": {
			"patterns": [
				{
					"captures": {
						"2": {
							"name": "variable.parameter.birdy"
						},
						"3": {
							"name": "variable.parameter.birdy"
						}
					},
					"match": "\\$((\\w+)|\\{([^{}]+)\\})"
				},
				{
					"match": "\\\\.",
					"name": "constant.character.escape.birdy"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"begin": "(?<!\\|r)\"",
					"end": "\"",
					"name": "string.interpolated.double.birdy",
					"patterns": [
						{
							"match": "\\n",
							"name": "invalid.string.newline"
						},
						{
							"include": "#string-interp"
						}
					]
				},
				{
					"begin": "r\"",
					"end": "\"",
					"name": "string.quoted.double.birdy",
					"patterns": [
						{
							"match": "\\n",
							"name": "invalid.string.newline"
						}
					]
				},
				{
					"begin": "(?<!\\|r)'",
					"end": "'",
					"name": "string.interpolated.single.birdy",
					"patterns": [
						{
							"match": "\\n",
							"name": "invalid.string.newline"
						},
						{
							"include": "#string-interp"
						}
					]
				},
				{
					"begin": "r'",
					"end": "'",
					"name": "string.quoted.single.birdy",
					"patterns": [
						{
							"match": "\\n",
							"name": "invalid.string.newline"
						}
					]
				}
			]
		},
		"punctuation": {
			"patterns": [
				{
					"match": ",",
					"name": "punctuation.comma.birdy"
				},
				{
					"match": "\n",
					"name": "punctuation.terminator.birdy"
				},
				{
					"match": "\\.",
					"name": "punctuation.dot.birdy"
				}
			]
		}
	},
	"scopeName": "source.birdy"
}